import negocio.IFiguras;

@AllArgsConstructor
@NoArgsConstructor
@Data

public class Cuadrado implements IFiguras{
	
	private double lado;

	@Override
	public double area() {
		return lado * lado;
	}

	@Override
	public double perimetro() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double volumen() {
		// TODO Auto-generated method stub
		return 0;
	}
	
	

}
